<div class="container wrapper">
  <a href="/ai/{{document.id}}">
    <p id="back" class="btn btn-primary">BACK</p>
  </a>
  <br><br>

  <h1>{{document.title}}</h1>

  <p id="rename" class="btn btn-primary">Rename</p>
  <br><br>


  <div id="editor">
  </div>

  <br><br>

  <button id="generate" class="btn btn-primary">Generate</button>
  <button id="save" class="btn btn-primary">Save</button>

  {{#if document.is_public}}
  <button id="make-private" class="btn btn-primary">Make Private</button>
  {{else}}
  <button id="make-public" class="btn btn-primary">Make Public</button>
  {{/if}}


  <br><br>

  <button id="delete" class="btn btn-primary">Delete</button>

</div>

<br><br>

<div id="mydiv">
  <!-- Include a header DIV with the same name as the draggable DIV, followed by "header" -->
  <div id="mydivheader">Click here to move</div>
    <p class="text-selected">Text Selected: </p>
    <label>Temperature: </label>
    <input id="temperature-control" type="text"> <br>
    <label>Max Token:</label>
    <input id="token-control" type="text"> <br>
  <button class="btn btn-primary">Gen+Insert at Cursor</button>
  <button class="btn btn-primary">Generate+</button>

  <p>this</p>
</div>


<script src="/js/editor.js"></script>
<script src="https://cdn.quilljs.com/1.3.6/quill.js"></script>
<script>
  var quill = new Quill('#editor', {
    theme: 'snow'
  });
</script>

<script src="https://cdn.jsdelivr.net/npm/dayjs@1/dayjs.min.js"></script>
<script>dayjs().format()</script>